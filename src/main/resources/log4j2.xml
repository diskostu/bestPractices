<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
  <Console name="LogToConsole" target="SYSTEM_OUT">
    <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
  </Console>

</Appenders>


<Loggers>
  <Logger name="de.axa.security.batch.rezertifizierung" level="debug" additivity="false">
    <AppenderRef ref="LogToConsole"/>
  </Logger>
  <Logger name="manual" level="debug" additivity="false">
    <AppenderRef ref="LogToConsole"/>
  </Logger>

  <Root level="info">
    <AppenderRef ref="LogToConsole"/>
  </Root>
</Loggers>
</Configuration>